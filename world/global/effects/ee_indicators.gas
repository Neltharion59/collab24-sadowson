///EMITTERS FOR THE QUEST AND SHOP ICONS.
///

///INDICATOR AMULETS
[effect_script*]
{
	name	=indicator_amulet;

	script	=[[

			sfx create fire #TARGET 
				"texture(b_gui_el_amulet)
				color0(0,0,0)
				color1(0,0,0)
				instant()
				velocity(0,0,0)
				accel(0,0,0)
				flamesize(1)
				fctrl(2,2,0)
				alphafade(0)
				count(1)
				min_count(1)
				radius_rmax(0)
				min_displace(10)
				max_displace(10)
				yorient()
				dark()
				offset(0,1.5,0)";
			
			sfx start #POP;

			]];
}
